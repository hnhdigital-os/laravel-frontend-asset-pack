{
    "name": "hnhdigital-os/laravel-frontend-asset-pack",
    "description": "Asset pack that uses hnhdigital-os/laravel-frontend-assets-loader",
    "keywords": ["laravel","assets", "frontend"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "require": {
        "php": ">=7.3",
        "hnhdigital-os/laravel-frontend-asset-loader": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "illuminate/database": "^7.0|^8.0",
        "symfony/process": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "HnhDigital\\LaravelFrontendAssetPack\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "process-timeout": 2000,
        "sort-packages": true,
        "platform": {
            "php": "7.3.0"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HnhDigital\\LaravelFrontendAssetPack\\ServiceProvider"
            ]
        }
    }
}
